bold = '\*\*[^\*]+\*\*'
italics = '\*[^\*]+\*'

# Inline links
state cbracket delim '\[(?=.*\]\()' '\)' begin
 usertype = '[^\]]+(?=\])'
 cbracket = '\]'
 cbracket = '\('
 include "_url.lang"
 include "_c_string.lang"
end

# Reference links for images
(symbol,symbol,string,symbol,normal,symbol,variable,symbol) =
`(!)(\[)([^\]]+)(\])([ ]{0,1})(\[)([^\]]*)(\])`


# Reference links
(symbol,string,symbol,normal,symbol,variable,symbol) =
`(\[)([^\]]+)(\])([ ]{0,1})(\[)([^\]]*)(\])`
