# @title Manifest
# @matchuti public.manifest-text
# @matchext mf
# @matchname Manifest, manifest

# language definition for Manifest files

include "_script_comment.lang"

comment start "!"

(type,normal,symbol) = `([^="+:;]+)([[:blank:]]*)(:=)`

include "_number.lang"
include "url.lang"

(string,symbol) = `(^[^:;]+)(:)`

regexp = '%([[:alnum:]]+)'